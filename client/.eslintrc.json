{
  "extends": [
    "google",
    "plugin:react/recommended",
    "plugin:prettier/recommended",
    "prettier",
    "prettier/babel",
    "prettier/react"
  ],
  "parser": "babel-eslint",
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "env": {
    "browser": true,
    "amd": true,
    "es2020": true,
    "node": true,
    "mocha": true
  },
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 2020,
    "sourceType": "module"
  },
  "plugins": [
    "babel",
    "react",
    "prettier"
  ],
  "rules": {
    "prettier/prettier": "warn",
    "require-jsdoc": "off",
    "no-eval": "error",
    "no-console": "off",
    "no-invalid-this": "off",
    "babel/no-invalid-this": "error"
  },
  "settings": {
    "react": {
      "version": "detect"
    }
  }
}